{
  "name": "teif-monorepo",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "preview": "turbo run preview",
    "test": "turbo run test",
    "test:ui": "turbo run test:ui",
    "test:coverage": "turbo run test:coverage",
    "test:e2e": "turbo run test:e2e",
    "test:e2e:ui": "turbo run test:e2e:ui",
    "test:all": "turbo run test:all",
    "clean": "turbo run clean && rm -rf node_modules packages/*/node_modules packages/*/.turbo",
    "format": "prettier --write \"packages/**/*.{ts,tsx,json,md}\"",
    "typecheck": "turbo run typecheck",
    "typecheck:frontend": "cd packages/frontend && pnpm typecheck",
    "typecheck:shared": "cd packages/shared && tsc --noEmit"
  },
  "devDependencies": {
    "prettier": "^3.0.0",
    "supabase": "^2.72.6",
    "turbo": "^2.0.0"
  },
  "dependencies": {
    "zod": "^4.3.5"
  }
}
